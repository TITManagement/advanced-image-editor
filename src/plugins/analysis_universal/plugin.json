{
  "name": "analysis",
  "display_name": "画像解析",
  "description": "高度な画像解析（ヒストグラム・特徴点・周波数・ノイズ・ブラー）を提供",
  "version": "1.0.0",
  "plugin_type": "analysis",
  "parameters": {
    "brightness_threshold": {
      "display_name": "明度閾値",
      "type": "slider", 
      "min": 0,
      "max": 255,
      "default": 128,
      "step": 1,
      "format": "{:.0f}"
    },
    "contrast_sensitivity": {
      "display_name": "コントラスト感度",
      "type": "slider",
      "min": 0.1,
      "max": 3.0,
      "default": 1.0,
      "step": 0.1,
      "format": "{:.1f}"
    }
  },
  "special_buttons": {
    "histogram": {
      "display_name": "ヒストグラム解析",
      "category": "ヒストグラム解析"
    },
    "rgb_histogram": {
      "display_name": "RGBヒストグラム",
      "category": "ヒストグラム解析"
    },
    "sift": {
      "display_name": "SIFT特徴点",
      "category": "特徴点検出"
    },
    "orb": {
      "display_name": "ORB特徴点",
      "category": "特徴点検出"
    },
    "dct": {
      "display_name": "DCT解析",
      "category": "周波数解析"
    },
    "fft": {
      "display_name": "FFT解析",
      "category": "周波数解析"
    },
    "noise": {
      "display_name": "ノイズ解析",
      "category": "品質解析"
    },
    "blur": {
      "display_name": "ブラー解析",  
      "category": "品質解析"
    }
  },
  "presets": {
    "標準": {
      "brightness_threshold": 128,
      "contrast_sensitivity": 1.0
    },
    "高感度": {
      "brightness_threshold": 100,
      "contrast_sensitivity": 1.5
    },
    "低感度": {
      "brightness_threshold": 150,
      "contrast_sensitivity": 0.7
    }
  }
}